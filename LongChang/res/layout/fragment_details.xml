<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent" 
    android:background="#ffffff">

    <com.hutu.longchang.widget.HeadTitleView
        android:id="@+id/trade_manager_head"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >
    </com.hutu.longchang.widget.HeadTitleView>
    
    <RelativeLayout 
        android:layout_below="@id/trade_manager_head"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_margin="10dp"
        android:background="#efefef">

       <RelativeLayout 
           android:id="@+id/rel1"
           android:layout_width="fill_parent"
           android:layout_height="wrap_content"
           android:layout_marginTop="10dp"
           android:layout_marginRight="10dp"
           android:layout_marginLeft="10dp"
           android:background="#ffffff">
           
           <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="类别:" 
            android:textSize="20sp"/>
           <TextView
            android:id="@+id/details_leibie_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/textView1"
            android:layout_toRightOf="@+id/textView1"
            android:text="TextView" />
           
           <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/details_leibie_txt"
            android:textSize="20sp"
            android:text="注册号：" />
           
           <TextView
            android:id="@+id/details_zhucehao_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/textView3"
            android:layout_toRightOf="@+id/textView3"
            android:text="TextView" />
       </RelativeLayout>
       <RelativeLayout 
           android:id="@+id/rel2"
           android:layout_below="@id/rel1"
           android:layout_width="fill_parent"
           android:layout_height="wrap_content"
           android:background="#ffffff"
           android:layout_marginTop="10dp"
           android:layout_marginRight="10dp"
           android:layout_marginLeft="10dp">
           <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="申请日期：" 
            android:textSize="16sp"/>
            <TextView
            android:id="@+id/details_shenqingriqi_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView5"
            android:layout_toRightOf="@+id/textView5"
            android:text="TextView" />
             <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/details_shenqingriqi_txt"
            android:text="注册日期:" 
            android:layout_toRightOf="@+id/details_shenqingriqi_txt"
            android:textSize="16sp"/>
             <TextView
            android:id="@+id/details_zhuceriqi_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView7"
            android:layout_toRightOf="@+id/textView7"
            android:text="TextView" />
       </RelativeLayout>
        <RelativeLayout 
            android:id="@+id/rel3"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
           android:layout_marginTop="10dp"
           android:layout_marginRight="10dp"
           android:layout_marginLeft="10dp"
           android:layout_below="@id/rel2">
            <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="商品名称：" 
            android:textSize="20sp"/>
             <TextView
                android:id="@+id/details_shangpinname_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="leibie"
                android:layout_toRightOf="@id/textView2"/>
        </RelativeLayout>
        <RelativeLayout 
            android:id="@+id/rel4"
            android:layout_below="@id/rel3"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:layout_marginTop="10dp"
           android:layout_marginRight="10dp"
           android:layout_marginLeft="10dp">
             <TextView
                 android:id="@+id/tesfs"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="使用商品："
                    android:textSize="20sp"/>
             
               <TextView
                    android:id="@+id/details_shiyongshangpin_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="leibie" 
                    android:layout_toRightOf="@+id/tesfs"/>
            
        </RelativeLayout>
        <RelativeLayout 
            android:id="@+id/rel5"
            android:layout_below="@+id/rel4"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:layout_marginTop="10dp"
           android:layout_marginRight="10dp"
           android:layout_marginLeft="10dp">
            
            <TextView
                android:id="@+id/leisitx"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="类似群组："
                android:textSize="20sp"/>
            <TextView
                android:id="@+id/details_leisiqunzu_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="leibie"
                 android:layout_toRightOf="@id/leisitx"/>
        </RelativeLayout>
        <RelativeLayout 
            android:id="@+id/rel6"
            android:layout_below="@id/rel5"
             android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:layout_marginTop="10dp"
           android:layout_marginRight="10dp"
           android:layout_marginLeft="10dp">
             <TextView
                 android:id="@+id/shenqingtxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="申请人：" 
                android:textSize="20sp"/>
             <TextView
                android:id="@+id/details_shenqingren_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="leibie"
                android:layout_toRightOf="@id/shenqingtxt"/>
        </RelativeLayout>
        <RelativeLayout 
            android:id="@+id/rel7"
            android:layout_below="@+id/rel6"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:layout_marginTop="10dp"
           android:layout_marginRight="10dp"
           android:layout_marginLeft="10dp">
            <TextView
                android:id="@+id/addtxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="申请地址："
                android:textSize="20sp"/>
             <TextView
                android:id="@+id/details_shenqingdizhi_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="leibie"
                android:layout_toRightOf="@id/addtxt" />
        </RelativeLayout>
        <RelativeLayout 
            android:id="@+id/rel8"
            android:layout_below="@+id/rel7"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:layout_marginTop="10dp"
           android:layout_marginRight="10dp"
           android:layout_marginLeft="10dp">
             <TextView
                 android:id="@+id/dailitxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="代理人："
                android:textSize="20sp"/>
            <TextView
                android:id="@+id/details_dailiren_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="leibie"
                android:layout_toRightOf="@+id/dailitxt"/>
        </RelativeLayout>
        <RelativeLayout 
            android:id="@+id/rel9"
            android:layout_below="@+id/rel8"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:layout_marginTop="10dp"
           android:layout_marginRight="10dp"
           android:layout_marginLeft="10dp">
            <TextView
                android:id="@+id/shangbiaotxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="商标状态："
                android:textSize="20sp"/>
             <TextView
                android:id="@+id/details_shangbiaozhuangtai_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="leibie"
                android:layout_toRightOf="@+id/shangbiaotxt"/>
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>